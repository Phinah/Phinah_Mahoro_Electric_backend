  "main": "./src/index.ts",
  "scripts": {
    "start": "nodemon ./src/index.ts",
    "build": "tsc --build ",
    "test": "nodemon ./src/index.ts",
    "dev": "nodemon ./src/index.ts",
    "prod": "tsc && node ./src/index.ts",
    "sequelize": "./node_modules/.bin/babel-node ./node_modules/.bin/sequelize $*",
